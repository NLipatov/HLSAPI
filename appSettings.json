{
  "Server": {
    "Port": 9001,
    "GetFileEndpointPostfix": "get?filename="
  },
  "Storage": {
    "MaxFileSizeMb": 300,
    "StorageFolderPath": "./storage"
  },
  "StorageDaemon": {
    "ShouldRun": true,
    "StorageLimitMinutes": 15,
    "StorageChecksIntervalMinutes": 5,
    "EnableLogging": true
  },
  "InfrastructureLayerConfiguration": {
    "FFMPEGConverter": {
      "UseLogging": true
    }
  }
}